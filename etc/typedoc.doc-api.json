{
  "$schema": "https://typedoc.org/schema.json",
  "includeVersion": true,
  "plugin": [
    "typedoc-plugin-keywords",
    "typedoc-plugin-katex",
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-mermaid",
    "typedoc-plugin-remove-references"
  ],
  "katex": {
    "options": {
      "delimiters": [
        {
          "left": "$$",
          "right": "$$",
          "display": true
        },
        {
          "left": "$",
          "right": "$",
          "display": false
        }
      ]
    }
  },
  "keywords": ["OpenTimestamps", "library"],
  "markedOptions": {
    "gfm": true
  },
  "json": "../dist/docs/api.json",
  "pretty": true,
  "sourceLinkExternal": true,
  "hideParameterTypesInTitle": false,
  "cleanOutputDir": true,
  "out": "../dist/docs/api",
  "entryPoints": ["../src/index.ts"],
  "hideGenerator": true,
  "searchInComments": true,
  "navigationLinks": {
    "La Crypta": "https://lacrypta.ar"
  },
  "visibilityFilters": {},
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false,
  "logLevel": "Verbose",
  "disableSources": false,
  "tsconfig": "./tsconfig.build.json",
  "customCss": "./typedoc-tweaks.css"
}
