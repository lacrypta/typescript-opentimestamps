{
  "name": "@lacrypta/typescript-opentimestamps",
  "version": "0.0.0",
  "description": "An OpenTimestamps client written in TypeScript",
  "keywords": [
    "OpenTimestamps"
  ],
  "repository": "github:lacrypta/typescript-opentimestamps",
  "homepage": "https://lacrypta.ar",
  "bugs": {
    "url": "https://github.com/lacrypta/typescript-opentimestamps/issues",
    "email": "bugs@lacrypta.ar"
  },
  "license": "AGPL-3.0-or-later",
  "author": {
    "name": "La Crypta",
    "url": "https://lacrypta.ar",
    "email": "info@lacrypta.ar"
  },
  "contributors": [],
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": {
    "./dist/cjs/index.js": "./browser/index-cjs.js",
    "./dist/esm/index.js": "./browser/index-esm.js"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/browser/index-esm.js",
      "node": "./dist/browser/index-cjs.js"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "mdlint": "markdownlint . .github -i node_modules -c ./etc/.markdownlint.jsonc",
    "mdlint:fix": "pnpm run mdlint --fix",
    "lint": "eslint src test --config ./etc/.eslintrc.json --no-error-on-unmatched-pattern",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier etc src test --config ./etc/.prettierrc.json --no-error-on-unmatched-pattern --check",
    "prettier:fix": "pnpm run prettier --write",
    "format": "pnpm run prettier:fix && pnpm run lint:fix && pnpm run mdlint:fix",
    "build": "pnpm run build:types && pnpm run build:browser && pnpm run build:esm && pnpm run build:cjs",
    "build:types": "tsc --build --verbose ./etc/tsconfig.build.json",
    "build:browser": "pnpm run build:browser:cjs && pnpm run build:browser:esm",
    "build:browser:cjs": "esbuild ./src/index.ts --outfile=./dist/browser/index.cjs.js --format=iife --bundle --packages=external --sourcemap=linked --sources-content=false --platform=node --global-name=opentimestamps --analyze",
    "build:browser:esm": "esbuild ./src/index.ts --outfile=./dist/browser/index.mjs.js --format=iife --bundle --sourcemap=linked --sources-content=false --platform=browser --global-name=opentimestamps --analyze",
    "build:esm": "esbuild $(find ./src -type f -name '*.ts' | xargs) --outdir=./dist/esm --format=esm --sourcemap=linked --sources-content=false --platform=neutral --analyze",
    "build:cjs": "esbuild $(find ./src -type f -name '*.ts' | xargs) --outdir=./dist/cjs --format=cjs --sourcemap=linked --sources-content=false --platform=node --analyze",
    "doc": "pnpm doc:api && pnpm run doc:internal",
    "doc:api": "typedoc --options ./etc/typedoc.api.json",
    "doc:internal": "typedoc --options ./etc/typedoc.internal.json",
    "clean": "rm -rf dist",
    "test": "jest --runInBand --config=./etc/jest.config.js",
    "reset": "pnpm clean && rm -rf node_modules && rm -f pnpm-lock.yaml"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "markdownlint-cli": "^0.39.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.7",
    "typedoc-plugin-katex": "^0.1.2",
    "typedoc-plugin-keywords": "^1.6.0",
    "typedoc-plugin-mdn-links": "^3.1.14",
    "typedoc-plugin-mermaid": "^1.10.0",
    "typedoc-plugin-remove-references": "^0.0.6",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "@noble/hashes": "^1.3.3"
  }
}
